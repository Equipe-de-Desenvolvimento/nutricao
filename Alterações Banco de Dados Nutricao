Dia 26/10/16


CREATE TABLE ponto.tb_estoque_parenteral_entrada
(
  estoque_entrada_parenteral_id serial NOT NULL,
  produto_id integer,
  estoque_saida_id integer,
  fornecedor_id integer,
  armazem_id integer,
  quantidade_entrada numeric,
  ativo boolean DEFAULT true,
  data_entrada timestamp without time zone,
  data_cadastro timestamp without time zone,
  operador_cadastro integer,
  validade date,
  data_atualizacao timestamp without time zone,
  operador_atualizacao integer,
  quantidade_saida numeric,
  CONSTRAINT tb_estoque_entrada_parenteral_pkey PRIMARY KEY (estoque_entrada_parenteral_id )
)


